<div class="control">
  <label
    for="<%=field.name%>"
    class="mb-2 block text-base font-medium text-slate-500 capitalize"
    ><%= field.label %></label
  >
  <input
    autocomplete="off"
    placeholder="<%=field.placeholder%>"
    type="<%=field.type%>"
    id="<%=field.name%>"
    name="<%=field.name%>"
    class="ring-1 ring-rose-200 focus:ring-2 focus:ring-rose-400 focus:shadow-sm rounded-sm w-full h-9 px-4 outline-none"
  />
</div>